#include <iostream>
#include <math.h>
#include <string>

using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	int A, B, C, D;
	cout << "Введите А=";
	cin >> A;
	cout << "Введите В=";
	cin >> B;
	cout << "Введите С=";
	cin >> C;
	D = A;
	A = B;
	B = C;
	C = D;
	cout << "A=" << A << endl;
	cout << "B=" << B << endl;
	cout << "C=" << C;
}
