#include <iostream>
#include <math.h>
#include <string>

using namespace std;

int main()
{
	setlocale (LC_ALL, "Russian");
	int A, B;
	cout << "Введите A ";
	cin >> A;
	cout << "Введите B ";
	cin >> B;
	if ((A > 2) and (B <= 3))
		cout << "Неравенства A>2 и B<=3 справедиливы ";
	else cout << "Неравенства A>2 и B<=3 не справедиливы ";
}
