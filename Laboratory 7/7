#include <iostream>
#include <math.h>
#include <string>

using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	int a, b, c, d, P;
	cout << "Введите значения a, b, c " << endl;
	cin >> a;
	cin >> b;
	cin >> c;
	if ((a<b+c) or (b<a+c) or (c<a+b))
		cout << "Треугольник с такими сторонами существует";
	else cout << "Треугольникa с такими сторонами не существует";
}
