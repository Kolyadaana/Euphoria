#include <iostream>
#include <math.h>
#include <string>

using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	int S, i, c, d = 0, k;

	cout << "Введите длину массивa A ";
	cin >> S;
	int* a = new int[S];
	for (int i = 0; i < S; i++)
	{
		cout << "a[" << i << "]=";
		cin >> a[i];
	}
	d = 0;
	for (i = 0;i < S - d; i++)
	{
		for (int j = i+1; j < S - 1 j++)
		{
			if (a[i] == a[j])
			{
				d += 1;
				for (j;j<S-d;j++)
					{
						k = a[j];
						a[j] = a[j + 1];
						a[j + 1] = a[j];
					}
				
			}
		}
	}
	for (i = 0; i < S - d; i++) cout << a[i] << " ";
}
