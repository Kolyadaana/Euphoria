#include <iostream>
#include <math.h>
#include <string>

using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	int a, b, i, j;
	cout << "Введите число A" << endl;
	cin >> a;
	cout << "Введите число B" << endl;
	cin >> b;
	i = a;
	j = 0;
	for (int i = a; i <= b; i++)
	{
		for (int j = 1; j <= i; j++)
			cout << i;
	}
}
