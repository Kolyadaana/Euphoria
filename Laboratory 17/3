#include <iostream>
#include <math.h>
#include <string>

using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	int S, i, c, b, k, w, K;
	cout << "K=";
	cin >> K;
	cout << "Введите длину массивa A ";
	cin >> S;
	int* a = new int[S];
	int* B = new int[S];
	int* C = new int[S];
	for (int i = 0; i < S; i++)
	{
		cout << "a[" << i << "]=";
		cin >> a[i];
	}
	b = 1;
	c = 0;
	int j = 0;
	for (i = 0; i < S; i++)
	{
		if (a[i] == a[i + 1])
		{
			b++;
		}
		else
		{
			B[j] = b;
			C[j] = a[i];
			b = 1;
			j++;
		}
	}
	w = B[K - 1];
	B[K - 1] = B[j-1];
	B[j-1] = w;
	w = C[K - 1];
	C[K - 1] = C[j-1];
	C[j-1] = w;
	w = 0;
	for (i=0; i<j-1; i++)
	{
		while (w < B[i]) 
		{
			cout << C[i];
			w++;
		}
		w = 0;
	}
}
